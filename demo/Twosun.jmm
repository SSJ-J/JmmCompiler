class Twosum{
    public void twosum(int[] nums, int target) {
        int i=0,j=0;
	while (i<target){
		while (j<target){
			if (nums[i]+nums[j]==target){
				print(i);
				print(j);
			}
			j=j+1;
		}
		i=i+1;
	}
    }
    public static void main() {
	int[] nums=new int[5]={1,2,3,4,5};
	int target=9;
	twosum();
    }
}